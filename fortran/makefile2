#LIBS = /usr/local/lib/lapack.a \
#       /usr/local/lib/libblas.a
test.exe: test.o
	gfortran -o test material.o test.o
	
material.o: material.f90
	gfortran -c material.f90
	
material.mod: material.f90
	gfortran -c material.f90
	
test.o: material.mod test.f90
	gfortran -c test.f90

clean:
	rm test.o test